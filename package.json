{
  "name": "anthropic-redis-memory-tool",
  "version": "0.1.0",
  "description": "Redis implementation for Anthropic Agent SDK memory tool handlers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "anthropic",
    "redis",
    "memory",
    "agent",
    "sdk",
    "tool"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.11.0",
    "eslint": "^9.0.0",
    "ioredis": "^5.9.2",
    "prettier": "^3.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.50.0",
    "ioredis": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    }
  }
}
